{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"]}],
    "eol-last": ["error", "never"],
    "no-console": 0,
    "no-useless-escape": 0,
    "comma-dangle": ["error", "never"]
  }
}